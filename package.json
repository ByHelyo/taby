{
  "name": "taby",
  "private": false,
  "version": "1.0.1",
  "description": "A tab switcher",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "publish": "chrome-webstore-upload-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ByHelyo/taby.git"
  },
  "author": "Franck Hu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ByHelyo/taby/issues"
  },
  "homepage": "https://github.com/ByHelyo/taby#readme",
  "devDependencies": {
    "@crxjs/vite-plugin": "^1.0.14",
    "@types/chrome": "^0.0.239",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "chrome-webstore-upload-cli": "^2.2.2",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "prettier": "2.8.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "fuse.js": "^6.6.2"
  }
}
